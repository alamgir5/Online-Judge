#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,h;
        cin>>n>>h;
        string s;
        for(int i=0; i<n; i++)
        {
            if(i<n-h)
                s+='0';
            else s+='1';
        }
        //cout<<s<<endl;
        do
        {
            cout<<s<<endl;
        }while(next_permutation(s.begin(),s.end()));
        if(t)
            cout<<endl;
    }
}
